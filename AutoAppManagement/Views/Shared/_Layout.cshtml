<body>
    <script>
        let userInfor = localStorage.getItem("userInfor"),
            userInforJson = userInfor ? JSON.parse(userInfor) : null;
        let _mainConfig = {
            userName: userInforJson && userInforJson.userName ? userInforJson.userName : '',
            avatar: userInforJson && userInforJson.avatar ? userInforJson.avatar : ''
        };
    </script>

    @if (Context.User.Identity.IsAuthenticated)
    {
        <header class="header">
            @await Html.PartialAsync("_LayoutHeader")
        </header>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/microsoft-signalr/6.0.1/signalr.js"></script>
        <script src="/js/Mixin/notification.js"></script>
    }
    else
    {
        <div class="landingcus">
            <header class="bg-blue container-fluid">
                @await Html.PartialAsync("_LayoutHeader")
            </header>
        </div>
    }



    @await Html.PartialAsync("_LayoutHeaderSecond")
    <main role="main" class="pb-3" style="display: flex; justify-content: center;">
        @RenderBody()
        <script src="~/js/Page/Base/detail.js"></script>

        @await Html.PartialAsync("ExtendComponent/PhoneRing")
        @await Html.PartialAsync("ExtendComponent/ChatSupport")
    </main>

    <footer class="border-top footer text-muted">
        @await Html.PartialAsync("_LayoutFooter")
    </footer>

    @await RenderSectionAsync("Scripts", required: false)
</body>
